        "        x = paste(var,\"of construction\")",
        "+ ",
        "      }else if(var == \"area\"){",
        "+ ",
        "        x = paste(var,\"in sqm\")",
        "+ ",
        "      }else{",
        "+ ",
        "        x = paste(\"net\", var, \"in Euro\")",
        "+ ",
        "      },",
        "+ ",
        "      y = \"Estimate Density\"",
        "+ ",
        "    ) +",
        "+ ",
        "    theme_minimal()",
        "+ ",
        "}",
        "> ",
        "> ",
        "# Create plots",
        "> ",
        "p1 <- hist(rent99, \"rent\")",
        "> ",
        "p2 <- hist(rent99, \"rentsqm\")",
        "> ",
        "p3 <- hist(rent99, \"area\")",
        "> ",
        "p4 <- hist(rent99, \"yearc\")",
        "> ",
        "> ",
        "# Display all",
        "> ",
        "library(gridExtra)",
        "> ",
        "grid.arrange(p1, p2, p3, p4, ncol = 2)",
