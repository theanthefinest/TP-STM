hist <- function(data, var) {
ggplot(data, aes(x = .data[[var]])) +
geom_histogram(aes(y = ..density..), bins = 30, color = "black", fill = "lightgray") +
geom_density(color = "red", linewidth = 0.5) +
labs(
title = paste("Hist. and Kernel Density:", var),
x = var,
y = "Density"
) +
theme_minimal()
}
# Load data
library(gamlss.data)
data(rent99)
rent99 <- as.data.frame(rent99)
rent99$rentsqm <- rent99$rent / rent99$area
# Create plots
p1 <- hist(rent99, "rent")
p2 <- hist(rent99, "rentsqm")
p3 <- hist(rent99, "area")
p4 <- hist(rent99, "yearc")
# Display all
library(gridExtra)
grid.arrange(p1, p2, p3, p4, ncol = 2)
ggplot(data, aes(x = .data[[var]])) +
geom_histogram(aes(y = ..density..), bins = 30, color = "black", fill = "lightgray") +
geom_density(color = "red", linewidth = 0.5) +
labs(
title = paste("Hist. and Kernel Density:", var),
x = paste("net",var, " in Euro"),
y = "Estimate Density"
) +
theme_minimal()
# Load required libraries
library(gamlss.data)
library(ggplot2)
library(gridExtra)
library(dplyr)
data(rent99)
rent99 <- as.data.frame(rent99)
# Rent for sqm ----------------->
rent99$rentsqm <- rent99$rent / rent99$area
# Define the variables to plot --------------->
vars <- c("rent", "rentsqm", "area", "yearc")
# Create a reusable function ----------------->
hist <- function(data, var) {
ggplot(data, aes(x = .data[[var]])) +
geom_histogram(aes(y = ..density..), bins = 30, color = "black", fill = "lightgray") +
geom_density(color = "red", linewidth = 0.5) +
labs(
title = paste("Hist. and Kernel Density:", var),
x = paste("net",var, " in Euro"),
y = "Estimate Density"
) +
theme_minimal()
}
# Create plots
p1 <- hist(rent99, "rent")
p2 <- hist(rent99, "rentsqm")
p3 <- hist(rent99, "area")
p4 <- hist(rent99, "yearc")
# Display all
library(gridExtra)
grid.arrange(p1, p2, p3, p4, ncol = 2)
# Load required libraries
library(gamlss.data)
library(ggplot2)
library(gridExtra)
library(dplyr)
data(rent99)
rent99 <- as.data.frame(rent99)
# Rent for sqm ----------------->
rent99$rentsqm <- rent99$rent / rent99$area
# Define the variables to plot --------------->
vars <- c("rent", "rentsqm", "area", "yearc")
# Create a reusable function ----------------->
hist <- function(data, var, x_name) {
ggplot(data, aes(x = .data[[var]])) +
geom_histogram(aes(y = ..density..), bins = 30, color = "black", fill = "lightgray") +
geom_density(color = "red", linewidth = 0.5) +
labs(
title = paste("Hist. and Kernel Density:", var),
if (var == "yearc"){
x = paste(var,"of construction")
}else if(var == "area"){
x = paste(var,"in sqm")
}else{
x = paste("net",var, "in Euro")
}
y = "Estimate Density"
