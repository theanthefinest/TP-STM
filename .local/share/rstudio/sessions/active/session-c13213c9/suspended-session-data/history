scatterplot <- function(data, var_x, var_y){
if (var_x == "area"){
x_label = "area in sqm"
}else if (var_x == "yearc"){
x_label = "year of construction"
}
ggplot(data, aes(x = .data[[var_x]], y = .data[[var_y]]))+
geom_point(alpha = 0.6, size = 1) +
labs(
title = "Net rent per sqm vs. area",
x = x_label,
y = "Net rent per sqm (Euro)"
) +
theme_minimal()
}
scatter1 <- scatterplot(rent99, "area", "rent")
scatter2 <- scatterplot(rent99, "rentsqm", "rent")
# =============================================================================>
# (b). Reconstruct the scatter plots below.
# =============================================================================>
scatterplot <- function(data, var_x, var_y){
if (var_x == "area"){
x_label = "area in sqm"
}else if (var_x == "yearc"){
x_label = "year of construction"
}else if (var_x == "rentsqm"){
x_label = "area in sqm"
}
ggplot(data, aes(x = .data[[var_x]], y = .data[[var_y]]))+
geom_point(alpha = 0.6, size = 1) +
labs(
title = "Net rent per sqm vs. area",
x = x_label,
y = "Net rent per sqm (Euro)"
) +
theme_minimal()
}
scatter1 <- scatterplot(rent99, "area", "rent")
scatter2 <- scatterplot(rent99, "rentsqm", "rent")
scatter3 <- scatterplot(rent99, "yearc", "rent")
print(scatter1)
print(scatter2)
scatter2 <- scatterplot(rent99, "rentsqm", "area")
# =============================================================================>
# (b). Reconstruct the scatter plots below.
# =============================================================================>
scatterplot <- function(data, var_x, var_y){
if (var_x == "area"){
x_label = "area in sqm"
}else if (var_x == "yearc"){
x_label = "year of construction"
}else if (var_x == "rentsqm"){
x_label = "area in sqm"
}
ggplot(data, aes(x = .data[[var_x]], y = .data[[var_y]]))+
geom_point(alpha = 0.6, size = 1) +
labs(
title = "Net rent per sqm vs. area",
x = x_label,
y = "Net rent per sqm (Euro)"
) +
theme_minimal()
}
scatter1 <- scatterplot(rent99, "area", "rent")
scatter2 <- scatterplot(rent99, "rentsqm", "area")
scatter3 <- scatterplot(rent99, "yearc", "rent")
print(scatter1)
print(scatter2)
